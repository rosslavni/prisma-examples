{
  "name": "prisma-circleci",
  "version": "1.0.0",
  "description": "Prisma CircleCI example",
  "author": "Waldemar Penner",
  "license": "MIT",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles",
    "prisma": "prisma"
  },
  "devDependencies": {
    "jest": "23.6.0",
    "ts-jest": "23.10.4",
    "typescript": "3.1.3"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "graphql-tools": "4.0.2",
    "prisma-client-lib": "1.19.0-beta.12"
  }
}
