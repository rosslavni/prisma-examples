{
  "name": "prisma-github-auth",
  "version": "1.0.0",
  "scripts": {
    "start": "dotenv -- nodemon -x ts-node -e ts,graphql src/index.ts",
    "debug": "dotenv -- nodemon -x 'ts-node --inspect' -e ts,graphql src/index.ts",
    "playground": "dotenv -- graphql playground",
    "build": "rm -rf dist && tsc"
  },
  "dependencies": {
    "graphql": "0.13.2",
    "graphql-import": "0.7.1",
    "graphql-yoga": "1.16.2",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.3.0",
    "prisma-binding": "1.5.19"
  },
  "devDependencies": {
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.7",
    "nodemon": "1.18.4",
    "prisma": "1.18.0",
    "ts-node": "7.0.1",
    "typescript": "3.1.2"
  },
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
